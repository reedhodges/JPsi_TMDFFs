Cell[TextData[{
 "The simplification for ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[Chi]", "\[Nu]\[Beta]"], TraditionalForm]],ExpressionUUID->
  "53caddbd-c316-43e6-baba-08f4a9e3c87b"],
 " is as follows.\n\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["\[Chi]", "\[Nu]\[Beta]"], " ", "=", " ", 
    RowBox[{
     RowBox[{"ubar", "(", 
      RowBox[{"P", "/", "2"}], ")"}], 
     SuperscriptBox["\[Gamma]", "\[Nu]"], 
     SuperscriptBox["T", "a"], 
     RowBox[{"v", "(", 
      RowBox[{"P", "/", "2"}], ")"}], 
     RowBox[{"vbar", "(", 
      RowBox[{"P", "/", "2"}], ")"}], 
     SuperscriptBox["\[Gamma]", "\[Beta]"], 
     SuperscriptBox["T", "a"], 
     RowBox[{"u", "(", 
      RowBox[{"P", "/", "2"}], ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "67dbadd8-e4fc-4c9f-9012-1cdcc53bf3bc"],
 "\nUse 9604237 A12b.\n= ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["M", "2"], 
    SubscriptBox[
     RowBox[{"(", 
      SuperscriptBox["L", "\[Nu]"], ")"}], "j"], 
    SuperscriptBox["\[Xi]", "+"], 
    SuperscriptBox["\[Sigma]", "j"], "\[Eta]", "  ", 
    SubscriptBox[
     RowBox[{"(", 
      SuperscriptBox["L", "\[Beta]"], ")"}], "i"], " ", 
    SuperscriptBox["\[Eta]", "+"], 
    SuperscriptBox["\[Sigma]", "i"], "\[Xi]"}], TraditionalForm]],
  ExpressionUUID->"afcbc261-8da8-4f3f-8187-8bb4c1acbfe3"],
 "\nUse 9604237 3.20.\n= ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      RowBox[{"(", 
       SuperscriptBox["L", "\[Nu]"], ")"}], "j"], 
     SubscriptBox[
      RowBox[{"(", 
       SuperscriptBox["L", "\[Beta]"], ")"}], "i"]}], " ", "<", 
    RowBox[{
     SuperscriptBox["\[Chi]", "+"], 
     SuperscriptBox["\[Sigma]", "i"], 
     SuperscriptBox["T", "a"], "\[Psi]", " ", 
     SubscriptBox["P", 
      RowBox[{
       RowBox[{"c", " ", "cbar"}], ",", 
       RowBox[{"c", " ", "cbar"}]}]], 
     SuperscriptBox["\[Psi]", "+"], 
     SuperscriptBox["\[Sigma]", "j"], 
     SuperscriptBox["T", "a"], "\[Chi]"}], ">"}], TraditionalForm]],
  ExpressionUUID->"dd9d857d-341c-4b6a-81df-28d8f81eedd8"],
 "\nUse 9604247 4.12b.\n= ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox[
      RowBox[{"(", 
       SuperscriptBox["L", "\[Nu]"], ")"}], "j"], 
     SubscriptBox[
      RowBox[{"(", 
       SuperscriptBox["L", "\[Beta]"], ")"}], "i"], " ", 
     FractionBox[
      RowBox[{"2", "M"}], "3"], 
     SubscriptBox["U", "\[Lambda]i"], 
     SubscriptBox[
      RowBox[{"(", 
       SuperscriptBox["U", "+"], ")"}], "j\[Lambda]"]}], "<", 
    RowBox[{
     SubscriptBox["O", "J\[Psi]"], "(", 
     RowBox[{"3", "S1", " ", "octet"}], ")"}], ">"}], TraditionalForm]],
  ExpressionUUID->"cb875e69-28e0-44d0-b9f7-03e9f375c3f3"],
 "\nThe boost matrices then boost the polarization vectors.\n= ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Epsilon]star", "\[Nu]"], 
     SuperscriptBox["\[Epsilon]", "\[Beta]"], " ", 
     FractionBox[
      RowBox[{"2", "M"}], "3"]}], "<", 
    RowBox[{
     SubscriptBox["O", "J\[Psi]"], "(", 
     RowBox[{"3", "S1", " ", "octet"}], ")"}], ">"}], TraditionalForm]],
  ExpressionUUID->"927f0afb-230d-4e2b-bbf6-e136c6b6844d"]
}], "Text",
 CellChangeTimes->{{3.8986856339977818`*^9, 3.8986859580318937`*^9}, {
  3.89868603777345*^9, 3.8986860429027557`*^9}, {3.898686104174655*^9, 
  3.8986862705442057`*^9}, {3.898686323844144*^9, 
  3.898686328077652*^9}},ExpressionUUID->"7c63da25-28f8-42eb-b488-\
72b91eb18da9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"quarkonium", " ", "polarization"}], " ", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"\[Epsilon]\[Epsilon]U", "[", 
     RowBox[{"\[Nu]_", ",", "\[Beta]_"}], "]"}], ":=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"-", "1"}], "3"], " ", 
      RowBox[{"MT", "[", 
       RowBox[{"\[Nu]", ",", "\[Beta]"}], "]"}]}], "+", 
     FractionBox[
      RowBox[{
       RowBox[{"FV", "[", 
        RowBox[{"P", ",", "\[Nu]"}], "]"}], 
       RowBox[{"FV", "[", 
        RowBox[{"P", ",", "\[Beta]"}], "]"}]}], 
      RowBox[{"3", 
       SuperscriptBox["M", "2"]}]]}]}], "\[IndentingNewLine]", 
   RowBox[{
